@model IEnumerable<HeathCheck1.Models.EspecialistaModel>
@{
    Layout = "_LayoutLogado";
}
@{
    ViewData["Title"] = "ListarEspecialista";
}

<h1>ListarEspecialista</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sobrenome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.registro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.telefone)
            </th>
           
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sobrenome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.registro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.telefone)
            </td>
          
           <td> 
          
             <form action="https://calendar.google.com/calendar/u/0/r/day" target="_blank">
                     <input type="submit" value="Agendar" class="btn btn-primary">
             </form>
            </td> 
        </tr>
}
    </tbody>
</table>
