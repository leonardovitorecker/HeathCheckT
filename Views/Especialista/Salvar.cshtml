@{
}
@model EspecialistaModel

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Heath.styles.css" asp-append-version="true" />
  

</head>



<div class="text-center">
<h1 class="display-4">Cadastro usuario</h1>
</div>

<form asp-controller="Especialista" asp-action="Create" method="post">
    

    <div class="form-group"  >
   <input type="text" asp-for="name" class="form-control" placeholder="Digite seu nome">
    </div>
    
    <div class="form-group"  >
   <input type="text" asp-for="email" class="form-control" placeholder="Digite seu email">
    </div>
    <div class="form-group"  >
   <input type="text" asp-for="senha" class="form-control" placeholder="crie sua senha">
    </div>
    <div class="form-group"  >
   <input type="text" asp-for="telefone" class="form-control" placeholder="crie sua telefone">
    </div>
      <div class="form-group"  >
   <input type="text" asp-for="registro" class="form-control" placeholder="Digite seu registro">
    </div>
    
    <div class="form-group"  >
   <input type="text" asp-for="sobrenome" class="form-control" placeholder="Digite seu sobrenome">
    </div>
     <div >
  
   <select name="especialidade" asp-action="BuscarEspecialidade">
      <% foreach(var in model)
      {%>
        <%=Html.actionLink"BuscarEspecialidade",new {id=item.id}  </%>

      }
     <option value=1 selected> selecione  a especialidade</option>

   <option value=2>item.nome</option>
   <option>43214123</option>
      
   </select>
   </div>
  
    
  
   
    <button type="submit" class="btn btn-primary">Cadastrar</button>
</form>

 <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
   
